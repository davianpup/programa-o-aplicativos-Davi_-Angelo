package com.mycompany.atividade10;

/**
 *
 * @author Aluno
 */
public class Aluno {
    private int id;
    private String nome;
    public Aluno(int id,String nome){
        this.id = id;
        this.nome = nome;
    }
    public Aluno(){}
    
    public void setId(int id){
        this.id = id;
    }
    
    public int getId(int id){
        return id;
    }
    
    public void setNome(String nome){
        this.nome = nome;
    }
    
    public String getNome(String nome){
        return nome;
    }
   
    
}

