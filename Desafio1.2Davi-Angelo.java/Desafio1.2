    public double CalcularDelta(int a,int b,int c){
        double calculo = (Math.pow(b,2))-(4*a*c);
        return calculo;
    }
    
    public int Soma(int n1, int n2){
        return n1+n2;
    }
    public int Subtracao(int n1, int n2){
        return n1-n2;
    }
    public int Multiplicacao(int n1, int n2){
        return n1*n2;
    }
    public int Divisao(int n1, int n2){
        return n1/n2;
    }
